generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  email     String   @unique
  name      String?
  createdAt DateTime @default(now())
  bio       String?
  publicId  Int      @unique @default(autoincrement())
  updatedAt DateTime @updatedAt
  id        String   @id @default(uuid()) @db.Uuid
}
